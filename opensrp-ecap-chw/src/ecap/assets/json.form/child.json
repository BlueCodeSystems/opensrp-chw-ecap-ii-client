  "step1": {
    "title": "Childs Details",
    "display_back_button": "true",
    "fields": [
      {
        "key": "instructions",
        "openmrs_entity_parent": "",
        "openmrs_entity": "concept",
        "openmrs_entity_id": "instructions",
        "type": "toaster_notes",
        "text": "Pediatric Surge Facility Index Mother Register",
        "text_color": "#000000",
        "label_text_style": "bold",
        "toaster_info_text": "This Register serves as a detailed record for Vulnerable Children and Adolescents (VCAs) aged 0-18 years identified through index tracing of Children/Adolescents of\nHIV+ mothers, offered and/or enrolled into the VCA comprehensive program. ",
        "toaster_info_title": "Introduction",
        "toaster_type": "info"
      },
      {
        "key": "household_id",
        "openmrs_entity_parent": "",
        "openmrs_entity": "person_attribute",
        "openmrs_entity_id": "household_id",
        "type": "edit_text",
        "read_only": "true",
        "hint": "Household ID",
        "edit_type": "name"
      },
      {
        "key": "unique_id",
        "openmrs_entity_parent": "",
        "openmrs_entity": "person_attribute",
        "openmrs_entity_id": "unique_id",
        "type": "edit_text",
        "read_only": "true",
        "hint": "VCA ID",
        "edit_type": "name"
      },
      {
        "key": "first_name",
        "openmrs_entity_parent": "",
        "openmrs_entity": "person",
        "openmrs_entity_id": "first_name",
        "type": "edit_text",
        "hint": "Child's first name ",
        "edit_type": "name",
        "look_up": "true",
        "v_required": {
          "value": true,
          "err": "Please enter First Name"
        }
      },
      {
        "key": "last_name",
        "openmrs_entity_parent": "",
        "openmrs_entity": "person",
        "openmrs_entity_id": "last_name",
        "type": "edit_text",
        "hint": "Child's last name ",
        "edit_type": "name",
        "look_up": "true",
        "v_required": {
          "value": true,
          "err": "Please enter last name"
        },
        "v_regex": {
          "value": "[A-Za-z\\s\\.\\-]*",
          "err": "Please enter a valid name"
        }
      },
      {
        "key": "adolescent_birthdate",
        "openmrs_entity_parent": "",
        "openmrs_entity": "person_attribute",
        "openmrs_entity_id": "adolescent_birthdate",
        "type": "date_picker",
        "hint": "Child's Date of birth",
        "min_date": "today-19y",
        "max_date": "today",
        "v_required": {
          "value": true,
          "err": "Please enter Birth Date"
        }
      },
      {
        "key": "myage",
        "openmrs_entity_parent": "",
        "openmrs_entity": "person_attribute",
        "openmrs_entity_id": "myage",
        "type": "edit_text",
        "hint": "Age",
        "hidden": "true",
        "edit_type": "number",
        "calculation": {
          "rules-engine": {
            "ex-rules": {
              "rules-file": "family_member_calculation.yml"
            }
          }
        }
      },
      {
        "key": "gender",
        "openmrs_entity_parent": "",
        "openmrs_entity": "person_attribute",
        "openmrs_entity_id": "gender",
        "type": "native_radio",
        "label": "Child's Gender",
        "options": [
          {
            "key": "male",
            "text": "Male"
          },
          {
            "key": "female",
            "text": "Female"
          }
        ],
        "v_required": {
          "value": true,
          "err": "Please Select Gender"
        }
      },
      {
        "key": "user_select_hiv",
        "openmrs_entity_parent": "",
        "openmrs_entity": "person_attribute",
        "openmrs_entity_id": "user_select_hiv",
        "type": "native_radio",
        "label_text_style": "bold",
        "text_color": "#000000",
        "label": "Was the child tested for HIV?",
        "options": [
          {
            "key": "yes",
            "openmrs_entity_parent": "",
            "openmrs_entity": "person_attribute",
            "openmrs_entity_id": "Yes",
            "text": "Yes"
          },
          {
            "key": "no",
            "openmrs_entity_parent": "",
            "openmrs_entity": "person_attribute",
            "openmrs_entity_id": "No",
            "text": "No"
          }
        ]
      },
      {
        "key": "is_hiv_positive",
        "openmrs_entity_parent": "",
        "openmrs_entity": "person_attribute",
        "openmrs_entity_id": "is_hiv_positive",
        "type": "native_radio",
        "label": "Is the Child/Adolescent HIV Positive",
        "options": [
          {
            "key": "yes",
            "text": "Yes"
          },
          {
            "key": "no",
            "text": "No"
          },
          {
            "key": "unknown",
            "text": "Unknown"
          }
        ],
        "v_required": {
          "value": true,
          "err": "This field is required"
        },
        "relevance": {
          "step1:user_select_hiv": {
            "type": "string",
            "ex": "equalTo(., \"yes\")"
          }
        }
      },
      {
        "key": "is_on_hiv_treatment",
        "openmrs_entity_parent": "",
        "openmrs_entity": "person_attribute",
        "openmrs_entity_id": "is_on_hiv_treatment",
        "type": "native_radio",
        "label": "Is the Child/Adolescent currently on HIV treatment?",
        "options": [
          {
            "key": "yes",
            "text": "Yes"
          },
          {
            "key": "no",
            "text": "No"
          }
        ],
        "relevance": {
          "step1:is_hiv_positive": {
            "type": "string",
            "ex": "equalTo(., \"yes\")"
          }
        }
      },
      {
        "key": "art_number",
        "openmrs_entity_parent": "",
        "openmrs_entity": "person_attribute",
        "openmrs_entity_id": "art_number",
        "type": "edit_text",
        "edit_type": "name",
        "hint": "ART Number",
        "relevance": {
          "step1:is_on_hiv_treatment": {
            "type": "string",
            "ex": "equalTo(., \"yes\")"
          }
        },
        "v_regex": {
          "value": "([0-9]{4}-[0-9]{4}-[0-9]{6}-[0-9])|\\s*",
          "err": "ART should be in the format of XXXX-XXXX-XXXXXX-X"
        }
      }
    ]
  }
}
